    "@mui/x-data-grid": "^8.7.0",
    "@next/bundle-analyzer": "^15.3.5",
    "@opentelemetry/sdk-trace-base": "^1",
    "@opentelemetry/sdk-trace-node": "^1",
    "@sentry/nextjs": "^9.35.0",
    "@types/lodash": "^4.17.20",
    "@types/papaparse": "^5.3.16",
    "cookie": "^1.0.2",
