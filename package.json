{
  "name": "@kausal/nzc-data-studio",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@11.4.2",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS=--inspect next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "graphql-codegen": "graphql-codegen --config src/config/codegen.ts"
